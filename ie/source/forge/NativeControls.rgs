HKCR
{
    NoRemove AppID
    {
        ForceRemove '%APPID%' = s 'Forge'
        {
            val DllSurrogate = s ''
        }   
        ForceRemove 'forge32.dll'
        {
            val AppID = s '%APPID%'
        }
        ForceRemove 'forge64.dll'
        {
            val AppID = s '%APPID%'
        }
    }
}

HKCR
{
	NoRemove CLSID
	{
		ForceRemove '%CLSID_NativeControls%' = s 'NativeControls Class'
		{
            val AppID = s '%APPID%'
			ForceRemove Programmable
			InprocServer32 = s '%MODULE%'
			{
				val ThreadingModel = s 'Single'
			}
			TypeLib = s '%LIBID_ForgeLib%'
			Version = s '1.0'
		}
	}
}
